.header {
  width: 100%;
  height: 350rpx;
  background: #F44336;
}
.header .status_bar {
    height: 25px;
    width: 100%;
}
.header .header-title {
    height: 114.58rpx;
    color: #fff;
    font-size: 41.67rpx;
    line-height: 114.58rpx;
    padding: 0 33.33rpx;
    box-sizing: border-box;
}
.header .message-search {
    width: calc(100% - 66.66rpx);
    height: 83.33rpx;
    position: relative;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 8.33rpx;
    margin-left: 33.33rpx;
}
.header .message-search .search-icon {
      width: 50rpx;
      height: 50rpx;
      position: absolute;
      left: 25rpx;
      top: 50%;
      margin-top: -25rpx;
}
.header .message-search .search-input {
      padding-left: 93.75rpx;
      box-sizing: border-box;
      height: 100%;
}
.header .message-search .search-input .uni-input-placeholder {
        color: #fff;
}
.header .message-tip {
    height: 70.83rpx;
    background: #ccc;
    margin-top: 35.42rpx;
    padding: 0 33.33rpx;
    box-sizing: border-box;
    border-top-left-radius: 50rpx;
    border-top-right-radius: 50rpx;
    background-color: #fff;
    line-height: 70.83rpx;
}
